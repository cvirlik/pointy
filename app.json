{
  "expo": {
    "name": "pointy",
    "slug": "pointy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "cover",
      "image": "./assets/images/splash-light.png",
      "dark": {
        "resizeMode": "cover",
        "image": "./assets/images/splash-dark.png"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cvirlik.pointy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cvirlik.pointy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0382829e-561f-4883-8da0-5ec640ca9643"
      }
    }
  }
}
